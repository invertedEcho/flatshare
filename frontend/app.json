{
  "expo": {
    "platforms": [
      "web",
      "android",
      "ios"
    ],
    "name": "WG-Tasks",
    "slug": "WG",
    "version": "1.0.0",
    "scheme": "wg-app",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.invertedEcho.wg_app",
      "intentFilters": [
        {
	  "_comment": "See https://docs.expo.dev/versions/latest/config/app/#autoverify",
	  "autoVerify": true,
          "action": "VIEW",
          "data": [
            {
              "scheme": "http",
              "host": "192.168.178.53"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "b05f4a7d-aeba-43e6-ac8c-38d70767e527"
      }
    }
  }
}
